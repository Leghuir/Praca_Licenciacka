@model MojDziennikv4.Models.Subject

@{
    ViewBag.Title = "Usuń przedmiot";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Usuń</h2>

<h3>Na pewno chcesz usunąć dany rekord?</h3>
<div>
    <h4>Przedmiot</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nazwa przedmiotu
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Subject_Name)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Usuń" class="btn btn-default" /> |
            <a href="/Subjects/Index"><input type="button" value="Powrót do listy" class="btn btn-default" /></a>
        </div>
    }
</div>
